<?xml version="1.0" encoding="UTF-8"?><!-- 圆角 主体背景  长方体 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/zise_normal" />
            <corners android:radius="2dp" />
            <stroke android:width="1dp" android:color="@color/zise_normal" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/transparent" />
            <corners android:radius="2dp" />
            <stroke android:width="1dp" android:color="@color/zise_normal" />
        </shape>
    </item>
</selector>